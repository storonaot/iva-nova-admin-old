{
  "kind": "collectionType",
  "collectionName": "photos",
  "info": {
    "singularName": "photo",
    "pluralName": "photos",
    "displayName": "Photo",
    "name": "photo"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "date": {
      "type": "date",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "description": {
      "type": "text",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "cover": {
      "type": "media",
      "allowedTypes": [
        "images"
      ],
      "multiple": false,
      "required": false
    },
    "dropbox_src": {
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "type": "string",
      "required": true,
      "regex": "^\\/([^?\\/]+)"
    }
  }
}
